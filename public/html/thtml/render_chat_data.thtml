<div class="comment" id="comment">
	<div class="label">
		<img src="{{image}}">
	</div>
	<div class="content">
		<a class="author">{{author}}</a>
		<div class="metadata">
			<div class="date">{{regist_date}}</div>
		</div>
		<div class="text">
			{{content}}
		</div>

		{{file}}

	</div>
</div>
