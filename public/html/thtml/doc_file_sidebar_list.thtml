<div class="event" id="doc_file_item">
	<a class="avatar">
		<img src="{{image}}" style="height : 35px !important;">
	</a>
	<div class="content">
		<div class="summary">
			<a class="user">{{member_nm}}</a> added file
			<div class="date">{{regist_date}}</div>
		</div>
		<div class="meta">
			<a class="file"><i class="file alternate outline icon"></i>{{file_nm}}</a>
		</div>
		<div class="meta">
			<a class="file" id="file_list_file_dowload">download</a>
		</div>
	</div>
</div>